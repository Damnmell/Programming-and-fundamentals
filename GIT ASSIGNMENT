def is_prime(n):
 for i in range(2, n // 2+1):
   if (n % i =0):
     return(0)
  return(1)
print("WELCOME")
print("For all the some of all prime numbers from one to another prime number")
x = int(input("Yor first integer here:"))
y =int(input("The last integer here:"))
result=[]
for all i in range(x, y + 1):
  if (is_prime(i)):
     result.append(i)
 print("The sum of all prime numbers is " + str(sum(result)))
